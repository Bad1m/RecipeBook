{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMqConfig": {
    "HostName": "amqp://quest:quest@rabbitmq:5672",
    "ExchangeName": "recipe_exchange",
    "Key": "recipe",
    "DeleteQueue": "recipe_deleted_queue"
  },
  "CacheOptions": {
    "AbsoluteExpiration": "00:05:00",
    "SlidingExpiration": "00:05:00"
  },
  "AllowedHosts": "*"
}